# make startup-files
# 13-Apr-94 G. Nikl
# 14-Apr-94 M. Fleischer clean+veryclean added

#### Start of system configuration section. ####

srcdir =	@srcdir@
VPATH =		@srcdir@

CC =		@CC@
CPP =           @CPP@
AS =		as

RANLIB =	@RANLIB@
AWK =           @AWK@

#### End system configuration section ####

OPTIONS=-I $(srcdir)/../headers -I ../../sources/headers -O -fomit-frame-pointer

%.o: %.c
	$(CC) $(OPTIONS) -c $^ 2>&1|tee $*.err
	-if test ! -s $*.err; then rm $*.err; fi
	echo "\$$$(V)" >>$@

.PHONY: all filelist subdirs clean veryclean

all: nrcrt0.o nbcrt0.o ncrt0.o libinit.o libinitr.o devinit.o

subdirs:

filelist:

clean:

veryclean:
	-rm *.o

# baserelative resident startup-code

nrcrt0.o: nrcrt0.S
	$(CC) -c $^
	echo "\$$$(V)" >>$@

# baserelative startup code

nbcrt0.o: nbcrt0.S
	$(CC) -c $^
	echo "\$$$(V)" >>$@

# normal (=large) startup code

ncrt0.o: ncrt0.S
	$(CC) -c $^
	echo "\$$$(V)" >>$@
